{
  "compilerOptions": {
    "allowJs": false,
    "incremental": true,
    "importHelpers": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": ["es6", "dom", "WebWorker"],
    "moduleResolution": "node",
    "module": "es2015",
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": false,
    "sourceMap": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}
